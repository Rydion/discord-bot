{
    "name": "discord-bot",
    "version": "1.0.0",
    "description": "Small discord bot",
    "private": true,
    "author": {
        "name": "Adrian Hintze",
        "email": "hintze.adrian@gmail.com",
        "url": "https://github.com/Rydion"
    },
    "engines": {
        "node": "10.10.0",
        "npm": "6.4.1"
    },
    "scripts": {
        "start": "node ./build/bot/Main.js",
        "full-deploy": "npm install && npm run deploy",
        "deploy": "npm run build && npm run forever",
        "prebuild": "rimraf ./build/bot/",
        "build": "tsc",
        "forever": "forever start ./build/bot/Main.js",
        "stop": "forever stopall"
    },
    "dependencies": {
        "@discordjs/uws": "10.149.0",
        "discord.js": "11.4.2",
        "erlpack": "0.1.2",
        "google-assistant": "0.5.2",
        "mkdirp": "0.5.1",
        "node-opus": "0.2.7",
        "rimraf": "2.6.2",
        "valid-url": "1.0.9",
        "zlib-sync": "0.1.4"
    },
    "devDependencies": {
        "@types/mkdirp": "0.5.2",
        "@types/node": "10.10.1",
        "@types/valid-url": "1.0.2",
        "typescript": "3.0.3"
    }
}
